@model StockDashboard.Tables.AppUsers;
@{
    ViewData["Title"] = "Register";
    var data = new StockDashboard.Tables.AppUsers();

}

<h1>Register with byte-trader</h1>

    <div class="container">
        <div class="row">
            <label for="inputUsername">Username</label>
            <input type="text" id="inputUsername" class="form-control" name="Username"  placeholder="Username" required autofocus>

            <label for="inputPassword">Password</label>
            <input type="text" id="inputPassword" class="form-control" name="Password" placeholder="Password" required autofocus>

            <label for="inputEmail">Email Address</label>
            <input type="text" id="inputEmail" class="form-control" name="Email" placeholder="Email Address" required autofocus>
        </div>
    </div>

    <div><h1>Alpaca</h1></div>


    <div class="container">
        <div class="row">
            <label for="apiKey">Api Key</label>
            <input type="text" id="apiKey" class="form-control" name="apiKey" placeholder="Api Key" required autofocus>

            <label for="secretKey">Secret Key</label>
            <input type="text" id="secretKey" class="form-control" name="secretKey" placeholder="Secret Key" required autofocus>

            <label for="paperApiKey">Paper Api Key</label>
            <input type="text" id="paperApiKey" class="form-control" name="paperApiKey" placeholder="Paper Api Key" required autofocus>

            <label for="paperSecretKey">Paper Secret Key</label>
            <input type="text" id="paperSecretKey" class="form-control" name="paperSecretKey" placeholder="Paper Secret Key" required autofocus>

            <button class="btn btn-primary" type="submit" name="submit" id="btnScore" onclick="RegisterNewUser()">
                Create Account
            </button>
        </div>

    </div>


